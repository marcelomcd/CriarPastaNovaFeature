# Copie para .env e preencha os valores.
# Ver docs/CONFIGURAR_PIPELINE.md para pipelines.

# Azure DevOps
AZURE_DEVOPS_ORG=qualiit
AZURE_DEVOPS_PROJECT=Quali IT - Inovação e Tecnologia
AZURE_DEVOPS_PAT=seu_pat_aqui

# SharePoint (Microsoft Entra ID)
SHAREPOINT_SITE_URL=https://qualiitcombr.sharepoint.com/sites/projetosqualiit
SHAREPOINT_FOLDER_PATH_BASE=Documentos Compartilhados/Projetos DevOps
SHAREPOINT_CLIENT_ID=seu_client_id
SHAREPOINT_CLIENT_SECRET=seu_client_secret
SHAREPOINT_TENANT_ID=seu_tenant_id

# Webhook (Service Hooks → POST /webhook/devops)
WEBHOOK_SECRET=um_secret_forte_para_validar_webhooks

# Opcional
LOG_LEVEL=INFO
